{
  "family": "aws-devops-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::619283292935:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "aws-devops-project",
      "image": "619283292935.dkr.ecr.us-east-1.amazonaws.com/aws-devops-project:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
